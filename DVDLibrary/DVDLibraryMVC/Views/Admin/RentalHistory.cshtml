@model DVDLibraryMVC.Models.RentalHistoryViewModel

<div class="row">
    <h3>Rental History</h3>
</div>
<div class="row">
    <table class="table table-striped table-hover">
        <thead>
        <tr>
            <th>#</th>
            <th>Name</th>
            <th>Title</th>
            <th>Serial Number</th>
            <th>Date Borrowed</th>
            <th>Date Returned</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var h in Model.History)
        {
            <tr>
                <th>@(Model.History.IndexOf(h)+1)</th>
                <th>@h.Name</th>
                <th>@h.Title</th>
                <th>@h.SerialNumberID</th>
                <th>@h.DateBorrowed</th>
                <th>@h.DateReturned</th>
            </tr>
        }
        </tbody>
    </table>
</div>